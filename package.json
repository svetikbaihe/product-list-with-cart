{
  "name": "product-list-with-cart",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite & (yarn run typed-scss:dev)",
    "build": "(yarn run typed-scss) & tsc --noEmit && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typed-scss": "typed-scss-modules src/**/*.module.scss --nameFormat camel --aliasPrefixes.@style src/assets/style/",
    "typed-scss:dev": "yarn typed-scss --watch",
    "lint:dev": "yarn prettier && yarn lint --fix",
    "prettier": "prettier ./src --write"
  },
  "dependencies": {
    "antd": "^5.20.5",
    "classnames": "^2.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "sass": "^1.78.0",
    "tailwindcss": "^3.4.10",
    "typed-scss-modules": "^8.0.1",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0",
    "vite": "^5.4.1"
  }
}
